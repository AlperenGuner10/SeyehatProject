@model ResetPasswordViewModel

@{
	Layout = null;
}
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css">
<div class="container">
	<br><br>
	<h1>Şifre Yenileme Sayfası</h1>
	<form method="post">
		<label>Şifrenizi Giriniz</label>
		<input type="password" class="form-control" asp-for="Password">
		<br>
		<label>Şifrenizi Tekrar Giriniz</label>
		<input type="password" class="form-control" asp-for="ConfirmPassword">
		<br>
		<button class="btn btn-primary">Şifre Güncelle</button>
	</form>
</div>