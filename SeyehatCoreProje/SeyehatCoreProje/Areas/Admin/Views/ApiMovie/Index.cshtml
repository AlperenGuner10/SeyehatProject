@model List<ApiMovieViewModel>
@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Api IMDB TOP 100 Film Listesi</h1>
<br>

<table class="table table-bordered">
	<tr>
		<th>#</th>
		<th>Film Adı</th>
		<th>IMDB Puanı</th>
		<th>Yapım Yılı</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.rank</td>
			<td>@item.title</td>
			<td>@item.rating</td>
			<td>@item.year</td>
		</tr>
	}
</table>


