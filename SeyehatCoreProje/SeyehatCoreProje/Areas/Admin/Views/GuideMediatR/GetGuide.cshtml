@model GetGuideByIDQueryResult
@{
	ViewData["Title"] = "GetGuide";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Rehber Bilgi Düzenleme Sayfası</h1>

<form method="post">
	<input type="hidden" asp-for="GuideID">
	<label for="">Rehber Adı</label>
	<input type="text" class="form-control" asp-for="Name">
	<br>
	<label for="">Rehber Açıklaması</label>
	<input type="text" class="form-control" asp-for="Description">
	<br>
	<button class="btn btn-outline-primary">Bilgileri Güncelle</button>
</form>
